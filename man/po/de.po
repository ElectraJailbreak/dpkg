msgid "B<-sk>"
msgstr "B<-sk>"

#. type: TP
#: dpkg-buildpackage.man dpkg-source.man
#, no-wrap
msgid "B<-su>"
msgstr "B<-su>"

#. type: TP
#: dpkg-buildpackage.man dpkg-source.man
#, no-wrap
